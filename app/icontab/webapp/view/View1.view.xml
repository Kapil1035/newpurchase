<mvc:View xmlns:form="sap.ui.layout.form" controllerName="icontab.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    	xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
    <form:SimpleForm >
     <Label text="Document Type:" ></Label>
    	<Select width="150px"
					>
					<core:Item  text="GPA"/>
					<core:Item  text="GPTA"/>
					<core:Item  text="GSA"/>
					<core:Item  text="GSSA"/>
				</Select>
       </form:SimpleForm>
       <Button text="GO" width="150px"   press="onItems"></Button>
    </Page>
</mvc:View>
